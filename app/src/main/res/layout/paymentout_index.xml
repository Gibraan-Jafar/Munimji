<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="Company name:"
        android:textColor="#000"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="14dp"
        android:layout_marginStart="14dp" />

    <AutoCompleteTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/et_cname_pout"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="39dp"
        android:layout_marginEnd="39dp"
        android:layout_toRightOf="@+id/btsubmitpout"
        android:layout_toEndOf="@+id/btsubmitpout">
    </AutoCompleteTextView>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Amount:"
        android:textColor="#000"
        android:layout_below="@+id/et_cname_pout"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/et_amount_pout"
        android:layout_alignTop="@+id/textView15"
        android:layout_alignLeft="@+id/et_cname_pout"
        android:layout_alignStart="@+id/et_cname_pout"
        android:layout_alignRight="@+id/et_cname_pout"
        android:layout_alignEnd="@+id/et_cname_pout"
        android:inputType="phone"/>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15"
        android:layout_below="@+id/rl_online_pout"
        android:layout_marginTop="20dp"
        android:text="Date:"
        android:textColor="#000" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/et_date_pout"
        android:layout_alignTop="@+id/textView16"
        android:layout_toRightOf="@+id/textView14"
        android:layout_toEndOf="@+id/textView14" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_date_pout"
        android:textSize="30dp"
        android:layout_alignBottom="@+id/et_date_pout"
        android:layout_toRightOf="@+id/et_date_pout"
        android:layout_toEndOf="@+id/et_date_pout"
        android:layout_alignTop="@+id/et_date_pout" />

    <Button
        android:id="@+id/btsubmitpout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="21dp"
        android:background="#eeffaa"
        android:text="Submit"
        android:layout_alignBaseline="@+id/btlistpout"
        android:layout_alignBottom="@+id/btlistpout"
        android:layout_toRightOf="@+id/textView16"
        android:layout_toEndOf="@+id/textView16"
        android:layout_marginStart="21dp" />

    <Button
        android:id="@+id/btlistpout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bt_date_pout"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"
        android:layout_toRightOf="@+id/btsubmitpout"
        android:background="#eeffaa"
        android:text="List" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mode "
        android:textColor="#000"
        android:layout_alignBaseline="@+id/bt_cheque_pout"
        android:layout_alignBottom="@+id/bt_cheque_pout"
        android:layout_alignLeft="@+id/textView15"
        android:layout_alignStart="@+id/textView15" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cash"
        android:id="@+id/bt_cash_pout"
        android:layout_alignBaseline="@+id/bt_online_pout"
        android:layout_alignBottom="@+id/bt_online_pout"
        android:layout_toLeftOf="@+id/bt_online_pout"
        android:layout_toStartOf="@+id/bt_online_pout" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cheque"
        android:id="@+id/bt_cheque_pout"
        android:layout_alignBaseline="@+id/bt_cash_pout"
        android:layout_alignBottom="@+id/bt_cash_pout"
        android:layout_toLeftOf="@+id/bt_cash_pout"
        android:layout_toStartOf="@+id/bt_cash_pout" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="online"
        android:id="@+id/bt_online_pout"
        android:layout_below="@+id/et_amount_pout"
        android:layout_toRightOf="@+id/btlistpout"
        android:layout_toEndOf="@+id/btlistpout" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rl_cheque_pout"
        android:layout_below="@+id/bt_cash_pout"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cheque number:"
            android:textColor="#000"
            android:layout_alignBaseline="@+id/et_check_pout"
            android:layout_alignBottom="@+id/et_check_pout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/et_check_pout"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_marginRight="22dp"
            android:layout_marginEnd="22dp" />


        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:text="Branch"
            android:textColor="#000"
            android:layout_below="@+id/et_check_pout"
            android:layout_alignLeft="@+id/tv43"
            android:layout_alignStart="@+id/tv43" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/et_branch_pout"
            android:layout_alignTop="@+id/textView19"
            android:layout_alignLeft="@+id/et_check_pout"
            android:layout_alignStart="@+id/et_check_pout"
            android:layout_alignRight="@+id/et_check_pout"
            android:layout_alignEnd="@+id/et_check_pout" />

        <TextView
            android:id="@+id/tv43"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cheque date:"
            android:textColor="#000"
            android:layout_alignBaseline="@+id/btcheckdated_pout"
            android:layout_alignBottom="@+id/btcheckdated_pout"
            android:layout_alignRight="@+id/textView18"
            android:layout_alignEnd="@+id/textView18" />

    <EditText
        android:id="@+id/et_checkdatepout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="6"
        android:layout_below="@+id/et_branch_pout"
        android:layout_alignLeft="@+id/et_branch_pout"
        android:layout_alignStart="@+id/et_branch_pout" />
        <Button
            android:id="@+id/btcheckdated_pout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/et_branch_pout"
            android:layout_toRightOf="@+id/et_checkdatepout"/>


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rl_online_pout"
        android:layout_below="@+id/rl_cheque_pout">

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/et_transid_pout"
            android:layout_alignBottom="@+id/et_transid_pout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="14dp"
            android:layout_marginStart="14dp"
            android:text="Treansaction ID:"
            android:textColor="#000" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/et_transid_pout"
            android:layout_marginRight="23dp"
            android:layout_marginEnd="23dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>



</RelativeLayout>